                            <uriMapper:UriMapping Uri="/LeaderAttend" MappedUri="/XiGouYiKuang/XiGouLeaderAttendRec.xaml"/>
                            <uriMapper:UriMapping Uri="/LeaderSchedule" MappedUri="/XiGouYiKuang/XiGouLeaderScheduleRec.xaml"/>
